{
  "name": "fec_menu_component",
  "version": "1.0.0",
  "description": "the menu component for our FEC project",
  "main": "src/server/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "./node_modules/.bin/webpack -d --watch",
    "build:prod": "./node_modules/.bin/webpack --config './webpack.prod.config.js'",
    "bundle": "./node_modules/.bin/webpack",
    "seed": "node seedDatabase.js",
    "server": "nodemon server.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thesilverspoon/menu-component.git"
  },
  "author": "MatGBX",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thesilverspoon/menu-component/issues"
  },
  "homepage": "https://github.com/thesilverspoon/menu-component#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.0",
    "mongo-mock": "^3.1.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "mongodb-memory-server": "^1.7.0",
    "mongoose": "^5.0.7",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "style-loader": "^0.20.2"
  }
